{
  "name": "best-scheduled-tweets",
  "version": "1.7.2",
  "description": "An opinionated library to extract the best links from scheduled tweets from a given Twitter account using Hootsuite and Facebook",
  "repository": {
    "type": "git",
    "url": "git@github.com:FullStackBulletin/best-scheduled-tweets.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "source .env && babel-node src/cli",
    "test:lint": "eslint src/**.js test/**.test.js",
    "test:unit": "nyc ava --tap test/**.test.js | tee .test_results.txt | tap-spec",
    "test": "npm run test:lint && npm run test:unit",
    "build": "rm -rf lib && NODE_ENV=production webpack && chmod +x lib/cli.js"
  },
  "bin": {
    "best-scheduled-tweets": "lib/cli.js"
  },
  "author": "Luciano Mammino",
  "license": "MIT",
  "engines": {
    "node": ">=4.3.2"
  },
  "dependencies": {
    "async": "^2.6.2",
    "debug": "^4.1.1",
    "fb": "^1.1.1",
    "meta-extractor": "^1.0.7",
    "moment": "^2.24.0",
    "normalize-url": "^1.9.0",
    "object-path": "^0.11.3",
    "object-values": "^1.0.0",
    "request": "^2.88.0",
    "twitter": "^1.7.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-async-generator-functions": "^6.22.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-es2015-destructuring": "^6.22.0",
    "babel-plugin-transform-es2015-function-name": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-es2015-parameters": "^6.22.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.22.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-es2015-sticky-regex": "^6.22.0",
    "babel-plugin-transform-es2015-unicode-regex": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-register": "^6.22.0",
    "codecov": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "nyc": "^13.3.0",
    "sinon": "^7.3.1",
    "tap-spec": "^5.0.0",
    "tap-xunit": "^2.3.0",
    "webpack": "^2.2.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
}
